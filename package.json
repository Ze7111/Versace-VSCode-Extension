{
  "name": "versace",
  "displayName": "Versace",
  "description": "Syntax Highlighting For Versace Code",
  "version": "1.0.2",
  "icon": "images/logo.ico",
  "publisher": "ZE7111",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ze7111/Versace-VSCode-Extension"
  },
  "engines": {
    "vscode": "^1.0.0",
    "visualstudio": "^1.0.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "contributes": {
    "languages": [{
      "id": "verscae",
      "aliases": ["versace", "verscae"],
      "extensions": [".v"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "verscae",
      "scopeName": "source.rust",
      "path": "./syntaxes/versace.tmLanguage.json"
    }]
  }
}
